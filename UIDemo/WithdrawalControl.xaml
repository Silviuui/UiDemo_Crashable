<UserControl x:Class="UiDemo.WithdrawalControl"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UiDemo"
             mc:Ignorable="d" 
             d:DesignHeight="550" d:DesignWidth="416">
    <Grid>
        <Label Content="Withdrawal transaction" HorizontalAlignment="Left" Margin="130,9,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Arial" FontWeight="Bold"/>
        <Label Content="Account #:" HorizontalAlignment="Left" Margin="45,40,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Arial" />
        <Label x:Name="accountnrlabel" Content="123" HorizontalAlignment="Left" Margin="115,40,0,0" VerticalAlignment="Top"  FontSize="14" FontFamily="Arial"/>
        <Label Content="Transaction #:" HorizontalAlignment="Left" Margin="235,40,0,0" VerticalAlignment="Top"  FontSize="14" FontFamily="Arial"/>

        <Label x:Name="transnrlabel" Content="123" HorizontalAlignment="Left" Margin="325,40,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Arial"/>

        <Label  Content="Check #1" HorizontalAlignment="Left" Margin="52,85,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Arial"/>

        <Label  Content="Check #2" HorizontalAlignment="Left" Margin="52,124,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Arial"/>

        <Label  Content="Total Withdrawal" HorizontalAlignment="Left" Margin="10,163,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Arial"/>
        <TextBox x:Name="check1" HorizontalAlignment="Left" Height="23" Margin="145,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextAlignment="Right" LostFocus="Textfield_Focus_Changed"/>
        <TextBox x:Name="check2" HorizontalAlignment="Left" Height="23" Margin="145,127,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextAlignment="Right" LostFocus="Textfield_Focus_Changed" />
        <Button x:Name="CashC" Content="Cash Count" HorizontalAlignment="Left" Margin="296,263,0,0" VerticalAlignment="Top" Width="80" Height="23" IsEnabled="False"/>
        <Label  Content="Cash out" HorizontalAlignment="Left" Margin="57,262,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Arial"/>
        <Label x:Name="TotalLB"  Content="$0.00" HorizontalAlignment="Left" Margin="220,262,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Arial"/>


        <Label  Content="Copyright UiPath. All rights reserved" HorizontalAlignment="Left" Margin="123,518,0,0" VerticalAlignment="Top" FontSize="10" FontFamily="Arial"/>
        <Button Content="Accept" HorizontalAlignment="Left" Margin="253,465,0,0" VerticalAlignment="Top" Width="80" Height="31" FontSize="12" FontFamily="Arial"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="71,465,0,0" VerticalAlignment="Top" Width="80" Height="31" FontSize="12" FontFamily="Arial"/>
        <Label x:Name="TotalLB2"  Content="$0.00" HorizontalAlignment="Left" Margin="220,171,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Arial"/>
    </Grid>
</UserControl>
